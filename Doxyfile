# macro names in the input files into their definition.
MACRO_EXPANSION        = NO

# If the EXPAND_ONLY_PREDEF and MACRO_EXPANSION tags are both set to YES
# then the macro expansion is limited to the macros specified with the
# PREDEFINED and EXPAND_AS_DEFINED tags.
EXPAND_ONLY_PREDEF     = NO

# The SEARCH_INCLUDES tag can be used to specify whether or not included files
# should be parsed just like normal input files.
SEARCH_INCLUDES        = YES

# The INCLUDE_PATH tag can be used to specify one or more directories that
# contain include files that are not input files but whose documentation
# is referenced by the input files.
INCLUDE_PATH           = ./src ./external/glfw/include ./external/glad/include ./external/assimp/include

# You can use the INCLUDE_FILE_PATTERNS tag to specify header files where the
# names contain specific patterns. One include file name can be matched by
# multiple patterns. Use INCLUDE_FILE_PATTERNS to specify one or
# more wildcard patterns (like *.h and *.hpp) to filter out header
# files names to be included. If left blank, Doxygen will look for files with
# extension *.h, *.hpp, *.hxx and *.h++. The patterns are case
# sensitive.
INCLUDE_FILE_PATTERNS  = *.h *.hpp

# The PREDEFINED tag can be used to specify one or more macro names that
# are defined before the preprocessor is started (similar to the -D option used
# with the C preprocessor).
PREDEFINED             = __cplusplus DOXYGEN_SKIP

# If the MACRO_EXPANSION and EXPAND_ONLY_PREDEF tags are set to YES then
# this tag can be used to specify a list of macro names that should be expanded.
EXPAND_AS_DEFINED      = 

# If the SKIP_FUNCTION_MACROS tag is set to YES (the default) then doxygen's
# preprocessor will remove all function-like macros that are alone on a line,
# have an all uppercase name, and do not end with a semicolon.
SKIP_FUNCTION_MACROS   = YES

#---------------------------------------------------------------------------
# Configuration options related to external references
#---------------------------------------------------------------------------

# The TAGFILES tag can be used to specify one or more tag files.
TAGFILES               = 

# When a file name is specified after GENERATE_TAGFILE, doxygen will create
# a tag file that is based on the input files it has parsed.
GENERATE_TAGFILE       = 

# If the ALLEXTERNALS tag is set to YES all external classes will be listed
# in the class index. If set to NO only the inherited external classes
# will be listed.
ALLEXTERNALS           = NO

# If the EXTERNAL_GROUPS tag is set to YES all external groups will be listed
# in the modules index. If set to NO, only the current project's groups will
# be listed.
EXTERNAL_GROUPS        = YES

# If the EXTERNAL_PAGES tag is set to YES all external pages will be listed
# in the related pages index. If set to NO, only the current project's
# pages will be listed.
EXTERNAL_PAGES         = YES

#---------------------------------------------------------------------------
# Configuration options related to the dot tool
#---------------------------------------------------------------------------

# If the CLASS_DIAGRAMS tag is set to YES (the default) Doxygen will
# generate a class hierarchy of all classes instead of a textual one.
CLASS_DIAGRAMS         = YES

# You can define message sequence charts within doxygen comments using the \msc
# command.  Doxygen will then run the mscgen tool (see
# http://www.mcternan.me.uk/mscgen/) to produce the chart and insert it in the
# documentation. The MSCGEN_PATH tag allows you to specify the directory where
# the mscgen tool is located. If left empty the tool is assumed to be found in
# the default search path.
MSCGEN_PATH            = 

# You can include diagrams made with dia in doxygen documentation. Doxygen will
# then run dia to produce the chart and insert it in the documentation.
DIA_PATH               = 

# If set to YES, the inheritance and collaboration graphs will include
# inheritance and usage relations if the destinations are undocumented or
# appear in a different project's documentation.
INCLUDE_GRAPH          = YES

# If the ENABLE_PREPROCESSING, SEARCH_INCLUDES, INCLUDE_GRAPH, and HAVE_DOT
# tags are set to YES then doxygen will generate a graph for each documented
# file showing the direct and indirect include dependencies of the file with
# other documented files.
INCLUDE_PATH_GRAPH     = YES

# If the CALL_GRAPH tag is set to YES and DOT_PATH is specified, a call dependency
# graph is generated for every global function or class method.
CALL_GRAPH             = NO

# If the CALLER_GRAPH tag is set to YES and DOT_PATH is specified, a caller
# dependency graph is generated for every global function or class method.
CALLER_GRAPH           = NO

# If the GRAPHICAL_HIERARCHY tag is set to YES then the graphical class hierarchy
# is included in the documentation.
GRAPHICAL_HIERARCHY    = YES

# If the DIRECTORY_GRAPH tag is set to YES then the graphical representation of
# the directories is included in the documentation.
DIRECTORY_GRAPH        = YES

# The DOT_IMAGE_FORMAT tag can be used to set the image format of the graphs
# generated by dot. For an explanation of the image formats see the section
# output formats in the documentation of the dot tool (Graphviz).
DOT_IMAGE_FORMAT       = png

# In case our dot version is not 2.26.0+ we need to specify the HASHCHAR
# character to doxygen (it has to be a single character). Doxygen will remove
# everything before and after this character from the ./DOT_GRAPH_MAX_NODES tag
# in order to prevent timing problems with very large graphs.
MSCGEN_PATH            = 

# The maximum dot graph depth is a safeguard against too large graphs
# becoming too memory intensive and grinding to a halt when running
# on a smaller machine. By default the maximum depth is set to 2.
# is mainly used by the "include dependency" graphs.
DOT_GRAPH_MAX_NODES    = 50

# Set the DOT_TRANSPARENT tag to YES to turn the background of the dot graphs
# white instead of transparent. This is useful if you want to include the graph
# in an RTF document that has a white background. Default value is NO.
DOT_TRANSPARENT        = NO

# Set the DOT_MULTI_TARGETS tag to YES allow dot to generate multiple output
# files in one go. Set it to NO if you use img src in your html files (may be
# needed for older versions of IE on Windows).
DOT_MULTI_TARGETS      = NO

# If the GENERATE_LEGEND tag is set to YES (the default) Doxygen will
# generate a legend page explaining the meaning of the various boxes and
# arrows in the DIRECTORY_GRAPH and GRAPHICAL_HIERARCHY graphs.
GENERATE_LEGEND        = YES

# If the DOT_CLEANUP tag is set to YES (the default) Doxygen will
# remove the intermediate dot files that are used to generate
# the various graphs.
DOT_CLEANUP            = YES
